{
  "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
  "editor.fontFamily": "FiraCode-Retina, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.renderWhitespace": "none",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.quickSuggestionsDelay": 200,
  "editor.suggest.localityBonus": true,
  "editor.hover.delay": 200,
  "search.searchOnTypeDebouncePeriod": 1000,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.bracketPairColorization.enabled": true,
  "editor.parameterHints.enabled": false,
  "workbench.editor.splitInGroupLayout": "horizontal",
  "editor.inlineSuggest.enabled": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "window.dialogStyle": "native",
  "editor.scrollbar.scrollByPage": true,
  "editor.suggest.preview": true,
  "window.commandCenter": false,
  "workbench.startupEditor": "none",
  "explorer.fileNesting.enabled": true,
  "window.autoDetectColorScheme": true,
  "git.openRepositoryInParentFolders": "never",
  "editor.suggestSelection": "first",
  "workbench.hover.delay": 200,
  "workbench.sash.hoverDelay": 200,
  "workbench.editor.empty.hint": "hidden",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "breadcrumbs.enabled": false,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 2,
  "editor.fontWeight": "500",
  "git.ignoreMissingGitWarning": true,
  "window.zoomLevel": 1,
  "workbench.layoutControl.enabled": true,
  "workbench.layoutControl.type": "both",
  "workbench.reduceMotion": "on",
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.wordWrap": "on",
  "terminal.integrated.fontWeight": "normal",
  "editor.renderControlCharacters": false,
  "editor.accessibilitySupport": "off",
  "workbench.editor.enablePreview": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "workbench.statusBar.visible": false,
  "editor.mouseWheelScrollSensitivity": 1,
  "editor.fastScrollSensitivity": 1,
  "editor.smoothScrolling": false,
  "editor.cursorBlinking": "phase",
  "editor.stickyScroll.enabled": true,
  "workbench.activityBar.location": "hidden",
  "workbench.editor.showTabs": "single",
  "terminal.integrated.fontSize": 16,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.hover.hidingDelay": 200,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb"
  },
  "editor.fontVariations": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.sideBar.location": "right",
  "files.autoSaveDelay": 100,
  "editor.lineNumbers": "off"
}
